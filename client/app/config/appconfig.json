                {
                    "indexname": "sensoroperationalmode",
                    "indexlabel": "Sensor Mode",
                    "indexvalues": "SM|IW|EW|WV",
                    "regex": ".*"

                },
                {
                    "indexname": "relativeorbitnumber",
                    "indexlabel": "Relative Orbit Number (from 1 to 175)",
