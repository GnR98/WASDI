                },
                {
                    "indexname": "producttype",
                    "indexlabel": "Product Type",
                    "indexvalues": "SLC|GRD|OCN",
                    "regex": ".*"

                },
                {
                    "indexname": "polarisationmode",
                    "indexlabel": "Polarisation",
                    "indexvalues": "HH|VV|HV|VH|HH+HV|VV+VH",
                    "regex": ".*"

